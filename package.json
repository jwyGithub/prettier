{
    "name": "@jiangweiye/prettier-config",
    "type": "module",
    "version": "0.0.11",
    "description": "A prettier config for jiangweiye",
    "author": "jiangweiye <jiangweiye@outlook.com> (https://github.com/jwyGithub)",
    "license": "SEE LICENSE IN LICENSE.md",
    "homepage": "https://github.com/jwyGithub/prettier",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "prettier",
        "config"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.esm.js",
            "require": "./dist/index.cjs.js"
        },
        "./options": {
            "types": "./dist/src/options.d.ts",
            "import": "./dist/src/options.esm.js",
            "require": "./dist/src/options.cjs.js"
        },
        "./format": {
            "types": "./dist/src/format.d.ts",
            "import": "./dist/src/format.esm.js",
            "require": "./dist/src/format.cjs.js"
        }
    },
    "typesVersions": {
        ".": {
            "*": [
                "./dist/index.d.ts"
            ],
            "options": [
                "./dist/src/options.d.ts"
            ],
            "format": [
                "./dist/src/format.d.ts"
            ]
        }
    },
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "format": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "optionalDependencies": {
        "prettier": "^3.2.5"
    },
    "devDependencies": {
        "@jiangweiye/eslint-config": "0.1.3-beta.1",
        "@jiangweiye/prettier-config": "workspace:*",
        "@jiangweiye/tsconfig": "^0.0.11",
        "@types/node": "^20.12.12",
        "eslint": "^9.3.0",
        "eslint-plugin-format": "^0.1.1",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5"
    },
    "sideEffects": false
}
